<div class="card-box genis-bg-color">
  <h2 class="header-title mt-0 mb-2 text-white">PCS</h2>

  <!-- <div class="widget-chart"> -->
  <div class="row">
    <div class="col-md-4">
      <div class="widget-detail text-left">
        <p class="genis-widget-title mb-1">operationMode</p>
        <h3 class="genis-widget-value mt-0 mb-2"><div class="spinner-grow text-success mr-1" role="status"></div>RUN </h3>
        <h3 class="genis-widget-value mt-0 mb-2"><div class="spinner-grow text-danger mr-1" role="status"></div>STOP </h3>
        <!-- <div class="spinner-grow text-success m-2" role="status"></div> -->
        <p class="genis-widget-title mb-1">chargingMode</p>
        <h3 class="genis-widget-value mt-0 mb-0">DisCharging</h3>
      </div>
    </div>
    <div class="col-md-8">
      <div class="row">
        <div class="col-md-6">
          <div class="genis-widget-circle mr-2">
            <div class="circle-txt-title">50</div>
            <div class="circle-txt-value">current</div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="genis-widget-circle mr-2">
            <div class="circle-txt-title">725.2</div>
            <div class="circle-txt-value">voltage</div>
          </div>
        </div>
        <div class="col-md-12">
          <div class="progress progress-bar-alt-primary progress-sm">
            <div
              class="progress-bar bg-primary"
              role="progressbar"
              aria-valuenow="77"
              aria-valuemin="0"
              aria-valuemax="100"
              style="width: 60%"
            >
              <span class="sr-only">77% Complete</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
